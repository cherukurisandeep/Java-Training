<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
 	http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.0.xsd
 	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

  <bean id="e" class="com.sb.springTraining.Employee">
    <constructor-arg value="10"  index="0"></constructor-arg>
    <constructor-arg value="Sandeep"  index="1"></constructor-arg>
  </bean>

    <bean name="employeeService" class="com.sb.springTraining.EmployeeService">
        <property name="employee" ref="e"></property>
    </bean>

    <bean id = "employeeData" class="com.sb.springTraining.Employee">
        <property name="id" value="11"></property>
        <property name="name" value="SaiSandeepCherukuri"></property>

    </bean>

    <bean id = "veh" class="com.sb.bean.inheritance.Vehicles">
        <property name="cars" value="Four wheels"></property>
        <property name="bikes" value="Two Wheels"></property>
    </bean>

    <bean id="brands" class="com.sb.bean.inheritance.Brands" parent="veh">
        <property name="bikes" value="Hero"></property>
        <property name="autos" value="Mahendra"></property>
    </bean>
</beans>